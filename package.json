{
  "name": "tiny-cookie",
  "version": "1.0.1",
  "description": "A tiny cookie manipulation plugin",
  "main": "lib/tiny-cookie.js",
  "module": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist lib coverage",
    "start": "babel-node server.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "karma start",
    "build:commonjs": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development rollup -c -i src/index.js -o dist/tiny-cookie.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -i src/index.js -o dist/tiny-cookie.min.js",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run clean && npm run lint && npm test && npm run build"
  },
  "author": {
    "name": "Alex Chao",
    "email": "alexchao1990@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Alex1990/tiny-cookie.git"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^1.0.2",
    "standard": "^10.0.2"
  },
  "keywords": [
    "cookie"
  ],
  "standard": {
    "ignore": [
      "dist/",
      "lib",
      "test/"
    ]
  }
}
